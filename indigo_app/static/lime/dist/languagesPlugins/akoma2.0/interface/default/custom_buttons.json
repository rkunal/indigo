	"commonElement": {
		"buttonStyle": "border-radius: 3px; background-color: #74BAAC;"
	},
	"commonReference": {
		"buttonStyle": "border-radius: 3px; background-color: #74BAAC;"
	},
	"commonJudgement": {
		"buttonStyle": "border-radius: 3px; background-color: #74BAAC;"
	},
	"commonReport": {
		"buttonStyle": "border-radius: 3px; background-color: #74BAAC;"
	}
